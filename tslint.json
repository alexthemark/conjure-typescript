{
  "defaultSeverity": "error",
  "extends": [
    "@blueprintjs/tslint-config"
  ],
  "rules": {
    "file-header": [true,
      "Copyright \\d{4} Palantir Technologies, Inc."
    ],
    "no-implicit-dependencies": false
  },
  "linterOptions": {
    "exclude": [
